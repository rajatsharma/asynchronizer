{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "import",
    "flowtype",
    "flowtype-errors"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": 0,
    "prefer-template": 0,
    "no-console": 0,
    "global-require": 0
  },
  "globals": {
    "Transformer": true
  }
}
